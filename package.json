{
  "name": "@push_innovation/aeg-swagger",
  "version": "0.0.1",
  "description": "Swagger client",
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "author": "Ad Exchange Group",
  "repository": {
    "type": "git",
    "url": "https://github.com/pushinnovation/aeg-swagger.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "test": "mocha --recursive --compilers ts:ts-node/register test/**/*.ts",
    "test:coverage": "nyc npm run test",
    "lint": "tslint src/**/*.ts",
    "lint:watch": "npm run lint && onchange \"**/*.ts\" \"tslint.json\" -- npm run lint",
    "compile": "tsc",
    "compile:watch": "npm run compile -- --watch",
    "dev": "npm run compile:watch",
    "build:test": "mocha --recursive --compilers ts:ts-node/register --reporter mocha-junit-reporter test/**/*.ts",
    "build:coverage": "nyc npm run build:test"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.8",
    "@types/config": "0.0.32",
    "@types/express": "^4.0.36",
    "@types/lodash": "^4.14.70",
    "@types/mocha": "^2.2.41",
    "@types/mustache": "^0.8.29",
    "@types/node": "^6.0.0",
    "@types/redis": "^2.6.0",
    "mocha": "^3.2.0",
    "mocha-junit-reporter": "^1.13.0",
    "nyc": "^11.0.3",
    "onchange": "^3.2.1",
    "should": "^11.2.0",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1"
  },
  "dependencies": {
    "@push_innovation/aeg-sdk": "0.0.4",
    "@push_innovation/aeg-security": "0.0.8",
    "@push_innovation/aeg-xray": "0.0.6",
    "bluebird": "^3.5.0",
    "config": "^1.26.1",
    "lodash": "^4.17.4",
    "mustache": "^2.3.0",
    "njwt": "^0.4.0",
    "redis": "^2.7.1"
  },
  "engine": "node >= 6.10.0"
}
